<?xml version="1.0"?>

<board type="Arduino Uno" name="TLC5973">
    <info>
        An example sketch for the TLC5973 device.
    </info>
    
    <devices>
        <device name="TLC5973" type="TLC5973">
            <setting id="num_leds" value="10"/>
            <setting id="pin" value="2"/>
        </device>
    </devices>
    <raat protocol="simple"></raat>
    <modules>
        <module>Task</module>
    </modules>
    <custom_code>
        <file>custom-code.cpp</file>
    </custom_code>
</board>
