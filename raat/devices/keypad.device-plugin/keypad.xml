<?xml version="1.0"?>

<board type="Arduino Uno" name="Keypad.Example">
    <info>
        An example sketch for the Keypad device
    </info>
    <devices>
        <device name="Keypad" type="Keypad">
            <setting id="buttons" value="123;456;789;*0#"/>
            <setting id="row_pins" value="5,6,7,8"/>
            <setting id="col_pins" value="2,3,4"/>
        </device>
    </devices>
    <raat protocol="simple" buffer_size="64">
    </raat>
</board>

