<?xml version="1.0"?>

<board type="Arduino Duemilanove" name="Adafruit NeoPixel.Example">
    <info>
        An example sketch for the Adafruit Neopixel device
    </info>
    
    <devices>
        <device name="NeoPixels" type="Adafruit NeoPixel">
            <setting id="pin" value="2"/>
            <setting id="npixels" value="30"/>
        </device>
    </devices>
    <raat protocol="simple"></raat>
</board>
