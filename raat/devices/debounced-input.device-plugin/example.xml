<?xml version="1.0"?>

<board type="Arduino Uno" name="Debounced Input">
    <info>
        An example sketch for the debounced input device
    </info>
    
    <devices>
        <device name="Button" type="Debounced Input">
            <setting id="pin" name="Input" value="5"/>
            <setting id="debounce_time" value="50"/>
            <setting id="pullup" value="true"/>
        </device>
    </devices>

    <custom_code>
        <file>example.cpp</file>
    </custom_code>
    
    <raat protocol="simple"></raat>
</board>
