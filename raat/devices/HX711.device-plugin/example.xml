<?xml version="1.0"?>

<board type="Arduino Uno" name="HX711.Example">
    <info>
        An example sketch for the HX711 device
    </info>
    
    <devices>
        <device name="Load Cell" type="HX711">
            <setting id="dout_pin" value="6"></setting>
            <setting id="sck_pin" value="7"></setting>
            <setting id="tare_at_boot" value="true"></setting>
        </device>
    </devices>

    <raat loglevel="RAAT_LOG_LEVEL_DEBUG">
    </raat>
</board>
