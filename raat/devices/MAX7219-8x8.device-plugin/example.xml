<?xml version="1.0"?>

<board type="Arduino Uno" name="MAX7219 8x8.Example">
    <info>
        An example sketch for the MAX7219 8x8 dot matrix device
    </info>
    
    <devices>
        <device name="MAX7219 Example" type="MAX7219 8x8">
            <setting id="cs_pin" value="6"/>
            <setting id="data_pin" value="7"/>
            <setting id="dclk_pin" value="5"/>
            <setting id="device_count" value="2"/>
        </device>
    </devices>

    <modules>
        <module>Task</module>
    </modules>
    <custom_code>
        <file>example.cpp</file>
    </custom_code>

    <raat protocol="simple"></raat>
</board>
