<?xml version="1.0"?>

<board type="Arduino Uno" name="Simple LED">
	<info>
		An example sketch to demonstrate what ADL can do
	</info>
	
	<devices>
		<device name="Onboard LED" type="Digital Output">
			<setting id="pin" name="LED" value="13"/>
		</device>
	</devices>
	<adl protocol="json"></adl>
</board>
