<?xml version="1.0"?>

<board type="Arduino Uno" name="Analog Outputs with reset">
	<info>
		An example sketch to demonstrate what ADL can do
	</info>
	
	<devices>
		<device name="LED1" type="Analog Output">
			<setting id="pin" name="LED_1_PIN" value="9"></setting>
		</device>
		<device name="LED2" type="Analog Output">
			<setting id="pin" name="LED_2_PIN" value="10"></setting>
		</device>
		<device name="LED3" type="Analog Output">
			<setting id="pin" name="LED_3_PIN" value="11"></setting>
		</device>
		<device name="Output Reset" type="Reset">
		</device>
	</devices>
	<adl protocol="simple"></adl>
</board>
