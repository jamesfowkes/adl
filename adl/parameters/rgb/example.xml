<?xml version="1.0"?>

<board type="Arduino Uno" name="RGB Parameter Example">
	<info>
		An example sketch for the RGB parameter
	</info>
	
	<parameters>
		<parameter name="My RGB Param" type="RGB">
			<setting id="clip" value="false"/>
			<setting id="min" value="0"/>
			<setting id="max" value="255"/>
		</parameter>
	</parameters>

	<custom_code>
		<file>custom_code.cpp</file>
	</custom_code>
</board>
