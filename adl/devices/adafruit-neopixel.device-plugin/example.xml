<?xml version="1.0"?>

<board type="Arduino Uno" name="Adafruit NeoPixel">
    <info>
        An example sketch for the Adafruit Neopixel device
    </info>
    
    <devices>
        <device name="NeoPixels" type="Adafruit NeoPixel">
            <setting id="pin" value="2"/>
            <setting id="npixels" value="21"/>
        </device>
    </devices>
    <adl protocol="simple"></adl>
</board>
