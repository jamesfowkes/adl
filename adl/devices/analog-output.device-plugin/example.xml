<?xml version="1.0"?>

<board type="Arduino Uno" name="Analog Output">
	<info>
		An example sketch for the analog output device
		Implements 3x PWM outputs, one of which is limited to 50% duty
	</info>
	
	<devices>
		<device name="LED1" type="Analog Output">
			<setting id="pin" name="LED_1_PIN" value="5"></setting>
		</device>
		<device name="LED2" type="Analog Output">
			<setting id="pin" name="LED_2_PIN" value="6"></setting>
		</device>
		<device name="LED3" type="Analog Output">
			<setting id="pin" name="LED_3_PIN" value="9"></setting>
			<setting id="low_limit" value="0"></setting>
			<setting id="high_limit" value="128"></setting>
		</device>
	</devices>
	<adl protocol="simple"></adl>
</board>
