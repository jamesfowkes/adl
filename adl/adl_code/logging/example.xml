<?xml version="1.0"?>

<board type="Arduino Uno" name="Logging Example">
    <info>
        An example sketch for logging
    </info>
    
    <custom_code>
        <file>example.cpp</file>
    </custom_code>
    
    <adl protocol="simple"></adl>

    <logging>
        <log_module>APPLICATION</log_module>
        <log_module>APP_AUTO_NUMERIC_PREFIX</log_module>
        <log_module prefix="ALT">APP_DIFFERENT_PREFIX</log_module>
        <log_module>ANOTHER_MODULE</log_module>
    </logging>

</board>
